{
  "name": "astro-paper",
  "version": "3.0.0",
  "private": false,
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build && jampack ./dist",
    "preview": "astro preview",
    "sync": "astro sync",
    "astro": "astro",
    "format:check": "prettier --plugin-search-dir=. --check .",
    "format": "prettier --plugin-search-dir=. --write .",
    "z": "cz",
    "prepare": "husky install",
    "lint": "eslint ."
  },
  "dependencies": {
    "@astrojs/check": "0.9.x",
    "@astrojs/rss": "4.x",
    "@resvg/resvg-js": "2.x",
    "astro": "5.x",
    "fuse.js": "7.x",
    "github-slugger": "2.x",
    "remark-collapse": "0.1.x",
    "remark-toc": "9.x",
    "satori": "0.15.x",
    "tailwindcss": "3.x",
    "typescript": "5.x"
  },
  "devDependencies": {
    "@astrojs/react": "4.x",
    "@astrojs/sitemap": "3.x",
    "@astrojs/tailwind": "6.x",
    "@divriots/jampack": "0.33.x",
    "@tailwindcss/typography": "0.5.x",
    "@types/github-slugger": "2.x",
    "@types/react": "19.x",
    "@typescript-eslint/parser": "8.x",
    "astro-eslint-parser": "1.2.x",
    "commitizen": "4.x",
    "cz-conventional-changelog": "3.x",
    "eslint": "9.x",
    "eslint-plugin-astro": "1.3.x",
    "husky": "9.x",
    "lint-staged": "16.x",
    "prettier": "3.x",
    "prettier-plugin-astro": "0.14.x",
    "prettier-plugin-tailwindcss": "0.6.x",
    "react": "19.x",
    "react-dom": "19.x"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md,mdx,json}": [
      "prettier --plugin-search-dir=. --write"
    ]
  }
}
